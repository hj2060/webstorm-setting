<application>
  <component name="CodeInsightSettings">
    <option name="completionCaseSensitive" value="1" />
    <option name="selectAutopopupSuggestionsByChars" value="true" />
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="COMPLETION_CASE_SENSITIVE" value="1" />
    <option name="SELECT_AUTOPOPUP_SUGGESTIONS_BY_CHARS" value="true" />
    <option name="AUTOINSERT_PAIR_BRACKET" value="false" />
  </component>
  <component name="DaemonCodeAnalyzerSettings" profile="joe" />
  <component name="EditorSettings">
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_BREADCRUMBS" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>